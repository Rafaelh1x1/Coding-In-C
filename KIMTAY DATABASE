USE KIMTAY;
CREATE TABLE SALES_REP
(
REP_ID CHAR (2) PRIMARY KEY,
FIRST_NAME CHAR (20),
LAST_NAME CHAR (20),
ADDRESS CHAR (20),
CITY CHAR (15),
STATE CHAR (2),
POSTAL CHAR (5),
CELL_PHONE CHAR (12),
COMMISSION DECIMAL (7, 2),
RATE DECIMAL (3, 2)
);
SHOW TABLES;
INSERT INTO SALES_REP (REP_ID, FIRST_NAME, LAST_NAME)
VALUES ('25', 'Donna', 'Sanchez');
SELECT*
	FROM SALES_REP;
UPDATE SALES_REP
	SET LAST_NAME = 'Salinas'
		WHERE REP_ID = '25';
DELETE
FROM SALES_REP
	WHERE REP_ID = '25';
DESCRIBE SALES_REP;
